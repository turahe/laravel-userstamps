{
    "name": "turahe/laravel-userstamps",
    "description": "Package to add created_by, updated_by and deleted_by to models.",
    "keywords": ["laravel", "created", "updated", "deleted", "userstamps"],
    "homepage": "https://github.com/turahe/laravel-userstamps",
    "license": "MIT",
    "authors": [
        {
            "name": "Nur Wachid",
            "email": "wachid@outlook.com",
            "homepage": "https://www.wach.id"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "doctrine/dbal": "^3.5",
        "laravel/pint": "^1.17"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Turahe\\UserStamps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turahe\\UserStamps\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turahe\\UserStamps\\UserStampsServiceProvider"
            ]
        }
    }
}
